import sys

def mochila_fuerza_bruta(pesos, precios, W):
    return




def generar_subconjuntos(pre, conj):
    if len(conj) == 1:
        return [pre + conj] + [pre]
    pre0 = pre[:]
    pre.append(conj[0])
    res = []
    # res.append( generar_subconjuntos(pre, conj[1:]))
    # res.append( generar_subconjuntos(pre0, conj[1:]))
    res = generar_subconjuntos(pre, conj[1:]) \
          + generar_subconjuntos(pre0,conj[1:])
    if pre0 == []:
        res.append([conj[0]])
    return res



n = int(sys.argv[1]) + 1
res = generar_subconjuntos([], range(1,n))
##res = generar_subconjuntos([], [0])
print("len: %d" % (n - 1))

for i,s in enumerate(res):
    print(": {}".format(s))
